{
	"name": "apollo-link-multipart",
	"version": "0.0.2",
	"description": "An Apollo link for multipart responses",
	"keywords": [
		"multipart",
		"multipart mixed",
		"fetch",
		"apollo",
		"stream",
		"defer"
	],
	"repository": "maraisr/apollo-link-multipart",
	"license": "MIT",
	"author": {
		"name": "Marais Rossouw",
		"email": "me@marais.dev",
		"url": "https://marais.io"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "types/multipartLink.d.ts",
	"files": [
		"types",
		"dist"
	],
	"scripts": {
		"build": "rollup -c",
		"format": "prettier \"{*,src/**/*,tests/**/*.github/**/*}.+(ts|yml|md)\" --write",
		"prepublishOnly": "yarn build",
		"test": "uvu -r ts-node/register -i mocks tests",
		"typecheck": "tsc --noEmit"
	},
	"prettier": {
		"bracketSpacing": true,
		"printWidth": 80,
		"proseWrap": "always",
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"dependencies": {
		"deepmerge": "^4.2.2",
		"meros": "^1.1.2",
		"nestie": "^1.0.0"
	},
	"devDependencies": {
		"@apollo/client": "3.3.9",
		"@marais/tsconfig": "0.0.1",
		"@rollup/plugin-node-resolve": "11.1.1",
		"@types/node": "14.14.26",
		"graphql": "^15.5.0",
		"graphql-tag": "^2.11.0",
		"prettier": "2.2.1",
		"rollup": "2.38.5",
		"rollup-plugin-typescript2": "0.29.0",
		"ts-node": "9.1.1",
		"typescript": "4.1.5",
		"uvu": "0.4.1",
		"zen-observable": "^0.8.15"
	},
	"peerDependencies": {
		"@apollo/client": ">=3.2"
	},
	"volta": {
		"node": "15.8.0",
		"yarn": "1.22.10"
	}
}
